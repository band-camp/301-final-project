<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../../partials/head') %>
</head>
<body>
    <header>
        <%- include('../../partials/header') %>
    </header>

    <main>

        <div>
          <% eventResults.forEach(event =>{ %>
            <ul id="bands">
              <div id="image">
                <img src="<%= event.image %>">
              </div>
              <li id="eventName"><%= event.eventName %></li>
              <li id="venue"><%= event.venue %></li>
              <li id="eventURL"><%= event.eventURL %></li>
              <li id="date"><%= event.date %></li>
              <li id="time"><%= event.startTime %></li>
            </ul>

          <% }) %>  
        </div>

    </main>

    <footer>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
        <script> 
          $('.band-item').on('click', function() {
            console.log(this.id);
          });
        </script>
    </footer>
    
</body>
</html>